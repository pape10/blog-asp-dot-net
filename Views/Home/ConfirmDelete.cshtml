
<html xmlns="http://www.w3.org/1999/xhtml">
@{
    ViewBag.Title = "Home";
}
<div>
    <div class="panel-heading">Are you sure you want to delete this blog?</div>
    <div class="panel-body"><a asp-action="SaveDelete" asp-controller="Home" asp-route-id="@Model"><button class="btn-danger btn-block">Delete</button></a></div>
    <div class="panel-body"><a asp-action="Index" asp-controller="Home"><button class="btn-info btn-block">Cancel</button></a></div>
</div>
